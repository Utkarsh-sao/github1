#include <iostream>
using namespace std;

int main() {
	int N,i;
	long int A[1000],max=0,max1=0;
	cin>>N;
	if(N>=2&&N<=1000)
	{
	      for(i=0;i<N;i++)
	      {
		cin>>A[i];
		if(max<A[i])
		{       max1= max;
			max = A[i];
		}
		else{
		if(max1<A[i]&&A[i]!=max)
			max1 = A[i];
		}
	      }
	     cout<<max1;
	}
	return 0;
}
